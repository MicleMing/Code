{
    "maxlength": 120,
    "indent": 4,
    "useTabIndent": false,
    "spaces": {
        "around": {
            "unaryOperations": false,
            "binaryOperators": true,
            "ternaryOperation": true
        },
        "before": {
            "functionDecalarationParentheses": false,
            "functionExpressionParentheses": true,
            "parenthess": true,
            "leftBrace": true,
            "keywords": true
        },
        "within": {
            "parenthess": false
        },
        "other": {
            "beforePropertyNameValueSeparator": false,
            "afterPropertyNameValueSeparator": true
        }
    },
    "bracesPlacement": {
        "functionDeclaration": 1,
        "other": 1
    },
    "blankLines": {
        "keepMaxBlankLines": 1,
        "atEndOfFile": true
    },
    "other": {
        "keepArraySingleLine": false
    }
    
}